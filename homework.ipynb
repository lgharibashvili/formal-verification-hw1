   "source": [
    "from z3 import *\n",
    "\n",
    "x, y = Ints('x y')\n",
    "\n",
    "s = Solver()\n",
    "s.add(ForAll([x, y], Implies(\n",
    "    And(x+y>-5, x-y<5),\n",
    "    Or(2*x-y<15, x+3*y>7)\n",
    ")))\n",
    "s.check()"
   ]
